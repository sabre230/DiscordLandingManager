<?xml version="1.0" encoding="UTF-8"?>
<xs:attribute name="IsEmbed" type="xs:boolean"/>

<Messages>
		 <!--
		For each message you want to send as part of an
		announcement, make a new <Message/> block and make sure 
		it has a block for <Text/> and <File/> inside.
		DLM doesn't handle Text and Files very well in the same
		message, so make a new Message block for each one.
		-->
	<Message IsEmbed="false">
		<Text>
			<!-- Place the text you want to send here -->
		</Text>

		<File>
			<!-- Place the file you want uploaded here -->
		</File>
	</Message>
	
</Messages>
